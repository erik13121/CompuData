<div id="table_background">
    <h2>Vehicle Bookings</h2>
    <div id="calendar"></div>
    <div id="backButton" onclick="window.location.href = '@Url.Action("Index", "SelectVehicleforVehicleService")'">Back</div>
</div>

<div id="myModal" class="modal fade" role="dialog">
    <div class="vertical-alignment-helper">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"><span id="eventTitle"></span></h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col text-center">
                            <button id="btnEdit" class="btn btn-default btn-sm pull-right" style="margin-right:5px;">
                                <span class="glyphicon glyphicon-pencil"></span> Edit Booking
                            </button>
                        </div>
                        <div class="col text-center">
                            <button id="btnAdjust" class="btn btn-default btn-sm pull-right" style="margin-right:5px;">
                                <span class="glyphicon glyphicon-pencil"></span> Adjust Odometer Reading
                            </button>
                        </div>
                        <div class="col text-center">
                            <button id="btnRemoveReading" class="btn btn-default btn-sm pull-right" style="margin-right:5px;">
                                <span class="glyphicon glyphicon-pencil"></span> Remove Odometer Reading
                            </button>
                        </div>
                        <div class="col text-center">
                            <button id="btnRemove" class="btn btn-default btn-sm pull-right" style="margin-right:5px;">
                                <span class="glyphicon glyphicon-pencil"></span> Remove Booking
                            </button>
                        </div>
                    </div>
                    <p id="pDetails"></p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="createEventModal" class="modal fade">
    <div class="vertical-alignment-helper">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content">
                <div class="modal-header">
                    <h4>Add a Vehicle Booking</h4>
                    <button type="button" class="close" data-dismiss="modal" onclick="document.getElementById('reasonError').style.visibility = 'hidden';
                        $('#bookingReason').removeClass('is-invalid');
                        $('#bookingReason').val('')">
                        <span aria-hidden="true">&times;</span> <span class="sr-only">close</span>
                    </button>
                </div>
                <div id="modalBody" class="modal-body">
                    <div class="form-group">
                        <input type="text" id="bookingReason" placeholder="Booking Reason" class="form-control" />
                        <span id="reasonError" style="color:red; visibility: hidden">Please enter a Reason</span>
                    </div>
                    <div class="row">
                        <div class="form-group col">
                            <label for="bookingStartTime">Start Time:</label>
                            <input type="text" id="bookingStartTime" class="form-control" />
                        </div>
                        <div class="form-group col pull-right">
                            <label for="bookingEndTime">End Time:</label>
                            <input type="text" id="bookingEndTime" class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col">
                            <label for="bookingStartTime">Project:</label>
                            @Html.DropDownList("Projects", (IEnumerable<SelectListItem>)ViewBag.Projects, new { id = "ddlProjects", @class = "form-control" })
                        </div>
                        <div class="form-group col pull-right">
                            <label for="bookingEndTime">Driver:</label>
                            @Html.DropDownList("Drivers", (IEnumerable<SelectListItem>)ViewBag.Users, new { id = "ddlUsers", @class = "form-control" })
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn" data-dismiss="modal" aria-hidden="true" onclick="document.getElementById('reasonError').style.visibility = 'hidden';
                        $('#bookingReason').removeClass('is-invalid');
                        $('#bookingReason').val('')">
                        Cancel
                    </button>
                    <button type="submit" class="btn btn-primary" id="submitButton">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>

