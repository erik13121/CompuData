    HttpContext.Current.Response.Cache.SetCacheability(HttpCacheability.NoCache);
}
<nav id="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="../MainMenu">Home</a></li>
        <li class="breadcrumb-item"><a href="../VehicleMenu">Vehicle Menu</a></li>
        <li class="breadcrumb-item"><a href="../VehicleBookings">Vehicle Bookings</a></li>
        <li class="breadcrumb-item"><a href="../SelectVehicleforVehicleService">Select Vehicle</a></li>
        <li class="breadcrumb-item active" aria-current="page">Add Vehicle Booking</li>
    </ol>
</nav>
@Html.Partial("_ProfilePartial")
@{Session["Referrer"] = System.IO.Path.GetFileName(Request.Url.AbsolutePath);
